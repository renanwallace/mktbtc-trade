{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [
      false
    ],
    "no-console": [
      false
    ],
    "object-literal-sort-keys": false,
    "interface-name": false,
    "member-access": [
      true,
      "no-public"
    ],
    "ordered-imports": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "no-shadowed-variable": false
  },
  "rulesDirectory": []
}